<h2>Login</h2>

<div class="container"><br>
  <%= bootstrap_form_tag url: login_path do %>
    <%= label_tag :email %>
    <%= text_field_tag :email, "user@hasnoname.com" %><br><br>
    <%= label_tag :password %>
    <%= password_field_tag :password, "user" %><br><br>
    <%= submit_tag "Enter", class: "btn btn-default" %>
  <% end %>
</div>